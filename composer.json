{
    "name": "tomatophp/filament-employees",
    "type": "library",
    "description": "Manage your employees with easy using Account builder and FilamentPHP\n\n",
    "keywords": [
        "php",
        "laravel",
        "employees",
        "hrms",
        "account",
        "management",
        "filament",
        "tomatophp"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\FilamentEmployees\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\FilamentEmployees\\FilamentEmployeesServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.1",
        "tomatophp/console-helpers": "^1.1"
    }
}
